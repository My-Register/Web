{
   "name": "web",
   "version": "0.0.0",
   "license": "MIT",
   "scripts": {
      "start": "nx serve",
      "build": "nx build",
      "test": "nx test && nx format",
      "commit": "cz",
      "prepare": "husky install"
   },
   "private": true,
   "dependencies": {
      "@nrwl/next": "13.3.6",
      "core-js": "^3.6.5",
      "express": "4.17.1",
      "mongoose": "^6.1.2",
      "next": "12.0.7",
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "regenerator-runtime": "0.13.7",
      "tslib": "^2.3.1"
   },
   "devDependencies": {
      "@commitlint/cli": "^15.0.0",
      "@nrwl/cli": "13.3.6",
      "@nrwl/cypress": "13.3.6",
      "@nrwl/eslint-plugin-nx": "13.3.8",
      "@nrwl/express": "13.3.7",
      "@nrwl/jest": "13.3.8",
      "@nrwl/linter": "13.3.4",
      "@nrwl/next": "13.3.6",
      "@nrwl/node": "13.3.2",
      "@nrwl/react": "13.3.6",
      "@nrwl/tao": "13.3.4",
      "@nrwl/web": "13.3.6",
      "@nrwl/workspace": "13.3.6",
      "@testing-library/react": "12.1.2",
      "@testing-library/react-hooks": "7.0.2",
      "@types/express": "4.17.0",
      "@types/jest": "27.0.2",
      "@types/node": "14.14.33",
      "@types/react": "17.0.30",
      "@types/react-dom": "17.0.9",
      "@typescript-eslint/eslint-plugin": "~5.3.0",
      "@typescript-eslint/parser": "~5.3.0",
      "babel-jest": "27.2.3",
      "commitlint-config-gitmoji": "^2.2.5",
      "cypress": "^8.3.0",
      "cz-conventional-changelog": "^3.3.0",
      "eslint": "8.2.0",
      "eslint-config-next": "12.0.7",
      "eslint-config-prettier": "8.1.0",
      "eslint-plugin-cypress": "^2.10.3",
      "eslint-plugin-import": "2.25.2",
      "eslint-plugin-jsx-a11y": "6.4.1",
      "eslint-plugin-react": "7.26.1",
      "eslint-plugin-react-hooks": "4.2.0",
      "husky": "^7.0.0",
      "jest": "27.2.3",
      "lint-staged": "^12.1.3",
      "prettier": "^2.3.1",
      "react-test-renderer": "17.0.2",
      "sass": "1.43.2",
      "ts-jest": "27.0.5",
      "typescript": "~4.4.3"
   },
   "linst-staged": {
      "*.{js,json,md,ts,tsx,jsx,css,scss,html}": [
         "npm format --uncommitted"
      ]
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog",
         "types": {
            "feat": {
               "title": ":sparkles: feat",
               "description": "‚ú® feat:\tAdding a new feature"
            },
            "fix": {
               "title": ":bug: fix",
               "description": "üêõ fix:\tFixing a bug"
            },
            "doc": {
               "title": ":memo: docs",
               "description": "üìù docs:\tAdd or update documentation"
            },
            "style": {
               "title": ":lipstick: style",
               "description": "üíÑ style:\tAdd or update styles, ui or ux"
            },
            "refactor": {
               "title": ":recycle: refactor",
               "description": "‚ôªÔ∏è  refactor:\tCode change that neither fixes a bug nor adds a feature"
            },
            "improve": {
               "title": ":zap: perf",
               "description": "‚ö°Ô∏è perf:\tCode change that improves performance"
            },
            "tests": {
               "title": ":white_check_mark: test",
               "description": "‚úÖ test:\tAdding tests cases"
            },
            "generation": {
               "title": ":truck: chore",
               "description": "üöö chore:\tChanges to the build process or auxiliary tools\n\t\tand libraries such as documentation generation"
            },
            "revert": {
               "title": ":rewind: revert",
               "description": "‚è™Ô∏è revert:\tRevert to a commit"
            },
            "work": {
               "title": ":construction: wip",
               "description": "üöß wip:\tWork in progress"
            },
            "build": {
               "title": ":construction_worker: build",
               "description": "üë∑ build:\tAdd or update regards to build process"
            },
            "build update": {
               "title": ":green_heart: ci",
               "description": "üíö ci:\tAdd or update regards to build process"
            }
         },
         "scopes": [
            {
               "name": "ui"
            },
            {
               "name": "android"
            },
            {
               "name": "ios"
            },
            {
               "name": "home"
            },
            {
               "name": "planner"
            },
            {
               "name": "settings"
            }
         ],
         "scopeOverrides": {
            "fix": [
               {
                  "name": "merge"
               },
               {
                  "name": "style"
               },
               {
                  "name": "test"
               },
               {
                  "name": "hotfix"
               }
            ]
         },
         "allowCustomScopes": true,
         "allowBreakingChanges": [
            "feat",
            "fix"
         ],
         "skipQuestions": [
            "body"
         ],
         "subjectLimit": 100
      }
   }
}
